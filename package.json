{
    "name": "@daveyplate/better-auth-tanstack",
    "homepage": "https://github.com/daveyplate/better-auth-tanstack",
    "version": "1.2.31",
    "description": "Tanstack hooks for better-auth",
    "scripts": {
        "build": "tsup --clean --dts",
        "dev": "tsc-watch --onSuccess 'biome check'",
        "prepublishOnly": "rm -rf dist && tsup --clean --dts"
    },
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./server": {
            "import": {
                "types": "./dist/server.d.ts",
                "default": "./dist/server.js"
            },
            "require": {
                "types": "./dist/server.d.cts",
                "default": "./dist/server.cjs"
            }
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "keywords": [
        "better-auth",
        "tanstack",
        "react-query",
        "tanstack-query",
        "query",
        "auth",
        "hooks",
        "prefetch",
        "ssr",
        "typescript"
    ],
    "author": "daveycodez",
    "license": "MIT",
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/node": "^22.13.10",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "next": "^15.2.2",
        "tsc-watch": "^6.2.1",
        "tsup": "^8.4.0",
        "tsx": "^4.19.3",
        "turbo": "^2.4.4",
        "typescript": "^5.8.2"
    },
    "peerDependencies": {
        "@tanstack/query-core": ">=5.65.0",
        "@tanstack/react-query": ">=5.65.0",
        "better-auth": ">=1.1.15",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "packageManager": "pnpm@10.2.1"
}
